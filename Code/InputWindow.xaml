<Window x:Class="CmdControl.InputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CmdControl"
        mc:Ignorable="d"
        Title="InputWindow" Height="76" Width="250" ResizeMode="NoResize" Icon="/Color_yr.ico" WindowStartupLocation="CenterScreen">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" Stroke="#FF5ABED6" StrokeThickness="2"/>
        <TextBox TextWrapping="Wrap" VerticalAlignment="Top" Margin="10,10,10,0"
                                     Text="{Binding old}"/>
    </Grid>
</Window>
